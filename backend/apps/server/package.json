    "helmet": "^8.1.0",
    "postgres": "^3.4.7",
    "superjson": "^2.2.2",
    "winston": "^3.17.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
